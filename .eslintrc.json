{
	"root": true,
	"env": {
		"es2022": true
	},
	"settings": {
		"import/resolver": {
			"webpack": {
				"config": "webpack.config.js"
			}
		}
	},
	"extends": [
		"eslint:recommended",
		"plugin:import/recommended",
		"airbnb-base",
		"plugin:jest/recommended",
		"prettier"
	],
	"rules": {
		"no-console": "off"
	},
	"ignorePatterns": ["/node_modules/", "/dist/"]
}
